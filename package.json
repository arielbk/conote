{
  "name": "conote",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server": "yarn workspace server start",
    "client": "HOST=0.0.0.0 yarn workspace client start",
    "start": "concurrently \"yarn client \" \"yarn server\""
  },
  "devDependencies": {
    "lerna": "^3.22.1",
    "concurrently": "^5.3.0"
  }
}
